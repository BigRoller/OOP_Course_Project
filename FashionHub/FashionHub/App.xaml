<Application x:Class="FashionHub.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FashionHub"
             StartupUri="MainWindow.xaml">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Resources/Converters/Converters.xaml"/>
        <ResourceDictionary Source="Resources/Resources.ru.xaml"/>
        <ResourceDictionary Source="Resources/Resources.en.xaml"/>
        <ResourceDictionary Source="Resources/Theme.Light.xaml"/>
        <ResourceDictionary Source="Resources/Theme.Dark.xaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
